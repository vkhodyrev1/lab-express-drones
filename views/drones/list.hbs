<h2>Available drones</h2>
<section class="drones">
    {{#each drones}}
    <h2>{{name}}</h2>
    <p>
        Number of propellers: {{propellers}} <br />
        Maximum drone speed: {{maxSpeed}} <br />
    </p>
    <a href="/drones/{{id}}/edit">Edit</a>
    <a href="/drones/{{id}}/delete">Delete</a>

    {{/each}}

</section>